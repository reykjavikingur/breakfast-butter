{
    "env": {
        "es6": true,
        "browser": true
    },
    "plugins": [
        "jsx-a11y"
    ],
    "extends": "airbnb",
    "rules": {
        "jsx-a11y/rule-name": 2,
        "semi": [
            "error",
            "always"
        ],
        "quotes": [
            "error",
            "double"
        ]
    }
}
